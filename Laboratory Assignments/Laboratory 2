SELECT * FROM CLIENT_MASTER;
SELECT * FROM PRODUCT_MASTER;
SELECT * FROM SALESMAN_MASTER;
SELECT * FROM SALES_ORDER;
SELECT * FROM SALES_ORDER_DETAILS;
/*----------------------------------------------------------------------------------------------------------------------------------------------------*/
-- Question 1
SELECT * FROM CLIENT_MASTER WHERE Name like '_a%'; /* Name whose second letter starts from a */
/*----------------------------------------------------------------------------------------------------------------------------------------------------*/
-- Question 2
SELECT * FROM CLIENT_MASTER WHERE CITY like '_a%'; /* Name of the city whose second letter is a*/
/*----------------------------------------------------------------------------------------------------------------------------------------------------*/
-- Question 3
SELECT * FROM CLIENT_MASTER WHERE CITY = 'Mumbai' or CITY = 'Bangalore';
/*----------------------------------------------------------------------------------------------------------------------------------------------------*/
-- Question 4
SELECT * FROM CLIENT_MASTER WHERE BALANCEDUE >= 10000;
/*----------------------------------------------------------------------------------------------------------------------------------------------------*/
-- Question 5
SELECT * FROM SALES_ORDER WHERE to_char(OrderDate,'Mon') = '01';
/*----------------------------------------------------------------------------------------------------------------------------------------------------*/
-- Question 6
SELECT * FROM SALES_ORDER WHERE ClientNo = 'C00001' OR ClientNo = 'C00002';
/*----------------------------------------------------------------------------------------------------------------------------------------------------*/
-- Question 7
SELECT * FROM PRODUCT_MASTER WHERE SellPrice > 2000 OR SellPrice <= 5000;
/*----------------------------------------------------------------------------------------------------------------------------------------------------*/
-- Question 8

/*----------------------------------------------------------------------------------------------------------------------------------------------------*/
-- Question 9
SELECT NAME,CITY,STATE FROM CLIENT_MASTER WHERE CITY NOT IN 'Maharashtra';
/*----------------------------------------------------------------------------------------------------------------------------------------------------*/
-- Question 10
SELECT * FROM PRODUCT_MASTER WHERE QtyInHand > ReorderLvl;
/*----------------------------------------------------------------------------------------------------------------------------------------------------*/
-- Question 11
